{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/info", "dest": "index.js" },
      { "src": "/temporaryinfo", "dest": "index.js" },
      { "src": "/users", "dest": "index.js" },
      { "src": "/(.*)", "dest": "index.js" }
    ]
  }
  